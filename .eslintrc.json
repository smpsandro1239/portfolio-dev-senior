{
  "env": {
    "browser": true,
    "es2020": true
  },
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "extends": ["eslint:recommended"],
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "off",
    "no-undef": "error",
    "no-redeclare": "error",
    "no-unreachable": "error",
    "prefer-const": "warn",
    "no-var": "warn"
  }
}